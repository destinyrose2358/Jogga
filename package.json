  "bugs": {
    "url": "https://github.com/destinyrose2358/Jogga/issues"
  },
  "homepage": "https://github.com/destinyrose2358/Jogga#readme",
  "dependencies": {
    "apollo-server": "^2.25.3",
    "aws-sdk": "^2.569.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.0",
    "cors": "^2.8.5",
    "create-react-app": "^3.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.5.8",
    "graphql-date": "^1.0.3",
    "graphql-tag": "^2.10.1",
    "graphql-upload": "^8.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.7.8",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "nodemon": "^1.19.4",
    "validator": "^12.0.0"
  }
}
